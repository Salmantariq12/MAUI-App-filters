<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAppFilter.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiAppFilter"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiAppFilter">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent 
        Title="Time Up" 
        ContentTemplate="{DataTemplate local:TimeUpPage}" 
        Route="TimeUpPage" />

</Shell>
